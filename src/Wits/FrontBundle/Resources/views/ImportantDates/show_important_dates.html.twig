
<!-- CONTACTS IMPORTANT DATE -->
<div class="panel panel-warning">
    <div class="panel-heading">
        <h3 class="panel-title">IMPORTANT DATES</h3>
    </div>
    <div class="panel-body list-group list-group-contacts">
        {% for imoprtDate in importantDates %}
        <a href="#" class="list-group-item">
            <span class="contacts-title">{{ imoprtDate.name }}</span>
            <p>{{ imoprtDate.date | date("M d, Y") }}</p>
        </a>
        {% else  %}
            <a href="#" class="list-group-item">
                <span class="contacts-title">No Importantes date for thi version yet</span>
            </a>
        {% endfor %}
    </div>
</div>
<!-- END IMPORTANT DATE -->