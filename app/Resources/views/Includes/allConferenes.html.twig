{% if allConferences != null %}
<!-- PREVIOUS-->
<div class="panel panel-warning">
    <div class="panel-heading">
        <h3 class="panel-title">PREVIOUS CONFERENCES</h3>
    </div>
    <div class="panel-body list-group list-group-contacts">
        {% for conference in allConferences %}
            <a href="{{ path('wits_front_homepage', {'edition': conference.edition})  }}" class="list-group-item">
                <span class="contacts-title">{{ conference.name }}</span>
                <p>{{ conference.startDate | date("Y") }}</p>
            </a>
        {% else %}
            <span class="contacts-title">NO PREVIOUS CONFERENCES</span>
        {% endfor %}
    </div>
</div>
<!-- END PREVIOUS WITS-->
{% endif %}